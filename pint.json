{
    "preset": "laravel",
    "rules": {
        "simplified_null_return": true,
        "braces": false,
        "new_with_braces": {
            "anonymous_class": false,
            "named_class": false
        },
        "align_multiline_comment": true,
        "array_syntax": true,
        "array_indentation": true,
        "blank_line_after_namespace": true,
        "fully_qualified_strict_types": true,
        "explicit_string_variable": true,
        "concat_space": true
    }
}
